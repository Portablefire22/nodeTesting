<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
<head>
    <%- include('../partials/head'); %>
</head>

<body>
    <header>
        <%- include('../partials/header'); %>
    </header>
        <main>
            <div class="centre">
                <h1>Projects</h1>
                <ul class="list-group list-group-flush">
                    <% for (var i = 0; i < projectInfo.length; i++) { %>
                        <li class="list-group-item">
                            <h3><a class="project" href=<%=projectInfo[i].InternalName%>><%= projectInfo[i].Name %></a></h3>
                            <p>Language(s): <%= projectInfo[i].Languages %></p>
                        </li>    
                    <% } %> 
                </ul>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>
</body>
</html>
